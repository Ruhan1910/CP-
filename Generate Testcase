#include<bits/stdc++.h>
using namespace std;
 
#define mx 200005
#define ll long long
#define mod 1000000007 //998244353
 

mt19937_64 rng(chrono::steady_clock::now().time_since_epoch().count());
ll my_rand(ll l, ll r) {
    return uniform_int_distribution<ll>(l, r)(rng);
}

vector<int>p;
 
void solve()
{
   int t=my_rand(1,100);
   cout<<t<<endl;
   while(t--){
      int x=my_rand(1,1e8);
      int y=my_rand(x+1,x+1e6);
      int z=my_rand(1,1e9);
      cout<<x<<" "<<y<<" "<<z<<endl;
   }
   

}
 
int main()
{
    int t=1;

    //scanf("%d",&t);
    while(t--)solve();
    return 0;
}
